{
  "name": "@savingsunited/zvezdochki",
  "version": "1.0.2",
  "description": "The component for easy creation of star rating components",
  "main": "dist/Zvezdochki.3283b0c3.js",
  "repository": "https://github.com/pcvg/zvezdochki",
  "author": "Savings United GmbH",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "del-cli dist/* && parcel build src/index.html --public-url /zvezdochki --no-content-hash --no-source-maps",
    "eslint": "./node_modules/.bin/eslint src",
    "stylelint": "./node_modules/.bin/stylelint src",
    "lint": "yarn eslint && yarn stylelint",
    "test": "nyc mocha --require @babel/register -r global-jsdom/lib/register tests && yarn report-coverage",
    "report-coverage": "nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "deploy": "yarn build && gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn build"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "canvas": "^2.6.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "global-jsdom": "^6.0.0",
    "husky": "^4.2.1",
    "jsdom": "^16.1.0",
    "microlight": "^0.0.7",
    "mocha": "^7.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.13.1",
    "nyc": "^15.0.0",
    "parcel": "^1.12.4",
    "postcss": "^7.0.26",
    "postcss-csso": "^4.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "stylelint": "^13.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-scss": "^3.14.2"
  },
  "dependencies": {
    "fingerprintjs2": "^2.1.0",
    "yarn": "^1.22.0"
  }
}
